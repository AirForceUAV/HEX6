<?xml version='1.0'?>
<config type='HEX6' FlightController='Pixhawk'>
    <entry index="0" name="Cloud">
        <description>Cloud param.</description>
        <param index="1" value="1">Whether is cloud module open? 0:close 1:open</param>
        <param index="2" value="queue.airforceuav.com">Cloud IP Adress</param>
        <param index="3" value="1883">Cloud Port Number </param>
        <param index="4" value="AirForceUAV">Cloud username. </param>
        <param index="5" value="AirForceUAV123">Cloud password. </param>     
    </entry>
     <entry index="1" name="vehicle">
        <description>vehilcle param.</description>
        <param index="1" value="1" unit="bool">Whether is vehicle open? 0:close 1:open</param>
        <param index="2" value="/dev/ttyACM0" unit="Device">Port .{'sitl' '/dev/ttyACM0' '/dev/ttyUSB0'}</param>
        <param index="3" value="115200" unit="Baudrate">Drone Port Number .such as 'sitl 115200' '/dev/ttyACM0 115200'</param>
    </entry>
    <entry index="2" name="Lidar">
        <description>Lidar module.</description>
        <param index="1" value="1" unit="bool">Whether is Lidar module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0" unit="Device">Serial port</param>
        <param index="3" value="2000" unit="mm">Vehicle Size. </param>
        <param index="4" value="3000" unit="mm">Safety Distance. </param>
    </entry>
    <entry index="3" name="Distance">
        <description>Default Distance.</description>
        <param index="1" value="3" unit="m">Default Altitude</param>
        <param index="2" value="1" unit="m">AIL Distance</param>
        <param index="3" value="1" unit="m">ROLL Distance</param>
        <param index="4" value="0.5" unit="m">ELE Distance</param>
    </entry>
    <entry index="4" name="Speed">
        <description>Default Speed.</description>
        <param index="1" value="3" unit="m/s">Guided Speed</param>
        <param index="2" value="1" unit="m/s">AIL Speed</param>
        <param index="3" value="1" unit="m/s">ROLL Speed</param>
        <param index="4" value="1" unit="m/s">ELE Speed</param>
        <param index="5" value="1" unit="m/s">Avoid Speed</param>
    </entry>
    <entry index="5" name="Angle">
        <description>Default angle.</description>
        <param index="1" value="10">Turn more angle in condition_yaw2().</param>
        <param index="2" value="3" >YAW angle.</param>
        <param index="3" value="3" >Ignore angle in Avoidance.</param>
    </entry>
    <entry index="6" name="Time">
        <description>Default time.</description>
        <param index="1" value="1" >Checktime in Avoid</param>
    </entry>
    <entry index="7" name="Way">
        <description>Default time.</description>
        <param index="1" value="2" >Guided with Avoid Way</param>
        <param index="2" value="2" >RTL    with Avoid Way</param>
    </entry>

</config>
