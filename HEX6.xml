<?xml version='1.0'?>
<config type='HEX6' FlightController='Pixhawk'>
    <entry index="0" name="Cloud">
        <description>Cloud param.</description>
        <param index="1" value="1">Whether is cloud module open? 0:close 1:open</param>
        <param index="2" value="queue.airforceuav.com">Cloud IP Adress</param>
        <param index="3" value="1883">Cloud Port Number </param>
        <param index="4" value="AirForceUAV">Cloud username. </param>
        <param index="5" value="AirForceUAV123">Cloud password. </param>     
    </entry>
     <entry index="1" name="vehicle">
        <description>vehilcle param.</description>
        <param index="1" value="1">Whether is vehicle open? 0:close 1:open</param>
        <param index="2" value="sitl">Port .{'sitl' '/dev/ttyACM0' '/dev/ttyUSB0'}</param>
        <param index="3" value="115200">Drone Port Number .such as 'sitl 115200' '/dev/ttyACM0 115200'</param>
    </entry>
    <entry index="2" name="Lidar">
        <description>Lidar module.</description>
        <param index="1" value="-1">Whether is Lidar module open? 0:close 1:open </param>
        <param index="2" value="/dev/ttyUSB0">Serial port</param>
        <param index="3" value="1500">Safety Distance. </param>
        <param index="4" value="3000">Detection Distance. </param>
    </entry>
</config>
